  CREATE TABLE tutorial.course_event_lesson_study
(
    `bcz_uid` UInt32,
    `user_id` UInt32,
    `sku_id` UInt32,
    `term_id` UInt32,
    `chapter_id` UInt32,
    `lesson_id` UInt32,
    `page_id` UInt32,
    `event_time` DateTime,
    `time_type` UInt16,
    `time` DateTime,
    `extra` String,
    `server_time` DateTime,
    `created_at` DateTime,
    `updated_at` DateTime,
    INDEX bcz_uid_idx bcz_uid TYPE set(0) GRANULARITY 1,
    INDEX term_id_idx term_id TYPE set(0) GRANULARITY 1
)
ENGINE = MergeTree
PARTITION BY (sku_id)
ORDER BY event_time
SETTINGS index_granularity = 8192